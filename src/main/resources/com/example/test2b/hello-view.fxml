<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.test2b.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Manraj" />
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="22043461" />
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="2024-06-19" />

    <Label fx:id="welcomeText" />
   <TextField fx:id="uname" promptText="UserName" />
   <TextField fx:id="pass" promptText="Password" />
    <Button onAction="#submit" text="Login" />
   <Label fx:id="message" />
    <Label fx:id="fail" />
</VBox>
